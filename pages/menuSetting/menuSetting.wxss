.menuSettingContainer{
    width: 100%;
    height: 100%;
    background: #f5f5f5;
}
.listGroupContainer{
    width: 100%;
    min-height: 200rpx;
}
.listItemContainer{
    width: 100%;
    height: 100rpx;
    background:  #fff;
    border-bottom: 1rpx solid #eee;
    display: flex;
    justify-content: space-between;
    align-items: center;
    box-sizing: border-box;
    padding: 0 20rpx;
}
.listItemContainer .mainContainer{
    width: 100%;
    height: 100%;
    display: flex;
    justify-content: flex-start;
    align-items: center;
    font-size: 32rpx;
    color: #333;
}
.listItemContainer .sideContainer{
    width: auto;
    height: 100%;
    flex-shrink: 0;
    display: flex;
    justify-content:flex-end;
    align-items: center;
}

.menuSwitch{
    height: 20rpx;
    width: 100rpx;
}
