.userContainer{
    width: 100%;
    height: 100%;
    background: #f5f5f5;
}
.listGroupContainer{
    width: 100%;
    height: auto;
}
.listGroupContainer .listItemContainer{
    width: 100%;
    height: 120rpx;
    background: #fff;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding:10rpx 20rpx;
    box-sizing: border-box;
    font-size: 32rpx;
    color: #666;
    border-bottom: 1rpx solid #eee;
    
}
.listGroupContainer .listItemContainer .content{
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
}
.userLogo{
    width: 80rpx;
    height: 80rpx;
    box-sizing: border-box;
    border-radius: 50%;
    overflow: hidden;
    border:1rpx solid #eee;
}
picker{
    width: 375rpx;
    height: 60rpx;
}
picker .picker{
    width: 100%;
    height: 100%;
    display: flex;
    justify-content: flex-end;
    align-items: center;
}